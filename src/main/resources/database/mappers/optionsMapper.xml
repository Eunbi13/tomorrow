<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tmh.t1.option.Options">
	<insert id="setOption" parameterType="OptionsVO" useGeneratedKeys="true" keyProperty="optionNum">
		INSERT into Options (kinds, NAME, price, ref, step) 
		VALUES (#{kinds},#{name},#{price},#{optionNum}, 0)
	</insert>
	

	
</mapper>

